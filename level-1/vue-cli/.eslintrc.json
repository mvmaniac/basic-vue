{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended",
    "prettier",
    "prettier/vue"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["vue", "import"],
  "rules": {
    "no-unused-vars": ["warn"]
  }
}
