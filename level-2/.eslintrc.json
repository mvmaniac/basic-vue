{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended",
    "plugin:prettier/recommended",
    "prettier/vue"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2020,
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "plugins": [
    "import",
    "vue",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": ["warn"]
  }
}
